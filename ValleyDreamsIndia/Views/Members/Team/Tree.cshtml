@model ValleyDreamsIndia.Common.TreeStructure.Parent
<div id="contentHeader">
    <h1>Binary Tree</h1>
</div> <!-- #contentHeader -->

<div class="container">
    <div class="grid-12">
        <div class="widget widget-table">

            <div class="widget-header">
                <span class="icon-list"></span>
                <h3 class="icon chart">Search User</h3>
            </div>

            <div class="widget-content">

                <table class="table table-bordered table-striped">
                    <thead>
                        <tr>
                            <th>User Id</th>
                            <th>
                                <input type="text" name="sui" id="sui" size="22" class="" 
                                       placeholder="Enter User Id" />

                            </th>



                        </tr>



                    </thead>


                </table>



            </div> <!-- .widget-content -->





            <p align="center" style="margin-top:40px;">
                <span>  <button class="btn btn-large btn-blue">Search</button>         </span>

                <span style="margin-left:10px;">  <button class="btn btn-large btn-gray">Cancel</button></span>

            </p>

        </div>





    </div>



    <div class="grid-12">
        <div class="widget widget-table">

            <div class="widget-header">
                <span class="icon-list"></span>
                <h3 class="icon chart">Parent Member Info</h3>
            </div>

            <div class="widget-content">

                <table class="table table-bordered table-striped">
                    <thead>

                        <tr>
                            <th>Sponser Id</th>
                            <th>A0065</th>
                        </tr>
                        <tr>
                            <th>User Id</th>
                            <th>A0066</th>


                        </tr>
                        <tr>
                            <th>Member Name</th>
                            <th>Amit Kumar</th>


                        </tr>

                        <tr>
                            <th>Left</th>
                            <th>0</th>


                        </tr>
                        <tr>
                            <th>Right</th>
                            <th>181</th>


                        </tr>
                        <tr>
                            <th>Directs</th>
                            <th>1</th>


                        </tr>







                    </thead>


                </table>


            </div> <!-- .widget-content -->





        </div>
    </div>

    <div class="grid-24">
        <div class="widget widget-table">

            <div class="widget-header">
                <span class="icon-compass"></span>
                <h3>Team Binary Tree</h3>
            </div> <!-- .widget-header -->


            <div class="widget-content">
                <p align="center" style="margin-right:10px;">
                    <img src="images/stream/defaultavatar_sm.png" />
                </p>

                <p align="center">
                    <button class="btn btn-primary "><span class="icon-loop"></span>Button</button>
                </p>

                <div style="display:inline;">

                    <span style="float:left;">
                        <img src="images/stream/defaultavatar_sm.png" />
                        <br>
                        <button style="margin-left:5px;" class="btn btn-primary "><span class="icon-loop"></span>Button</button>
                    </span>





                    <span style="float:right;">
                        <img src="images/stream/defaultavatar_sm.png" />
                        <br>


                        <button style="margin-left:5px;" class="btn btn-primary "><span class="icon-loop"></span>Button</button>
                    </span>
                </div>
            </div> <!-- .widget-content -->

        </div>


    </div>


</div>

